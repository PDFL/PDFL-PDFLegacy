{
  "manifest_version": 3,
  "name": "PDF - Legacy",
  "version": "1.0.7",
  "description": "Detects and opens pdfs in the PDF - Legacy reader",
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["*://*/*.pdf"],
      "js": ["pdfPopup.js"],
      "css": ["injectedStyle.css"]
    }
  ]
}
